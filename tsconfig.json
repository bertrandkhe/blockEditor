{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["es2017", "dom"],
    "module": "es2020",
    "target": "es2017",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "mui-edit/*": ["src/*"]
    },
    "esModuleInterop": true,
    "declaration": true
  },
  "include": [
    "src/**/*",
    "dev/**/*"
  ],
  "exclude": [
    "webpack.config.js"
  ]
}
